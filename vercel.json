{
    "version": 2,
    "builds": [
        {
            "src": "backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/frontend/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/frontend/js/$1"
        },
        {
            "src": "/(.*).html",
            "dest": "/frontend/$1.html"
        },
        {
            "src": "/(register|login|getBalance)",
            "dest": "/backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/backend/server.js"
        }
    ]
}